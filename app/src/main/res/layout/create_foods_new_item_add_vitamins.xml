<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/primaryLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <TextView
        android:id="@+id/addVitamins_tv_addVit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Add Vitamins"
        android:textSize="20dp"

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"

        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.025" />

    <ImageButton
        android:id="@+id/addVitamins_back_button"
        android:layout_width="76dp"
        android:layout_height="45dp"
        android:background="#00000000"

        app:layout_constraintBottom_toBottomOf="@id/addVitamins_tv_addVit"

        app:layout_constraintEnd_toStartOf="@+id/addVitamins_tv_addVit"
        app:layout_constraintHorizontal_bias="0.25"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/ic_arrow_back_black_24dp"
        />
    <ImageButton
        android:id="@+id/addVitamins_break"
        android:layout_width="47dp"
        android:layout_height="48dp"
        android:background="#00000000"
        android:src="@drawable/ic_clear_black_24dp"

        app:layout_constraintBottom_toBottomOf="@+id/addVitamins_tv_addVit"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.9"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>

    <TextView
        android:id="@+id/tv_addVitamins_servingData"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="--serving size--"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/addVitamins_tv_addVit"
        app:layout_constraintVertical_bias="0.05"/>

    <TextView
        android:id="@+id/tv_addVitamins_vitA"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:text="vitamin A:"
        android:textSize="18dp"
        android:paddingVertical="8dp"

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="@+id/vit_guideline"
        app:layout_constraintTop_toBottomOf="@+id/addVitamins_tv_addVit"

        app:layout_constraintVertical_bias="0.25"/>

    <TextView
        android:id="@+id/tv_addVitamins_vitC"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:paddingVertical="8dp"

        android:text="vitamin C:"
        android:textSize="18dp"

        app:layout_constraintStart_toStartOf="@+id/vit_guideline"
        app:layout_constraintTop_toBottomOf="@+id/vit_dividerOne"/>
    <TextView
        android:id="@+id/tv_addVitamins_vitD"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:paddingVertical="8dp"

        android:text="vitamin D:"
        android:textSize="18dp"

        app:layout_constraintStart_toStartOf="@+id/vit_guideline"
        app:layout_constraintTop_toBottomOf="@+id/vit_dividerTwo"/>
    <TextView
        android:id="@+id/tv_addVitamins_vitE"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:paddingVertical="8dp"

        android:text="vitamin E:"
        android:textSize="18dp"

        app:layout_constraintStart_toStartOf="@+id/vit_guideline"
        app:layout_constraintTop_toBottomOf="@+id/vit_dividerThree"/>

    <TextView
        android:id="@+id/tv_addVitamins_vitK"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:paddingVertical="8dp"

        android:text="vitamin K:"
        android:textSize="18dp"

        app:layout_constraintStart_toStartOf="@+id/vit_guideline"
        app:layout_constraintTop_toBottomOf="@+id/vit_dividerFour" />
    <TextView
        android:id="@+id/tv_addVitamins_vitB6"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:paddingVertical="8dp"

        android:text="vitamin B6:"
        android:textSize="18dp"

        app:layout_constraintStart_toStartOf="@+id/vit_guideline"
        app:layout_constraintTop_toBottomOf="@+id/vit_dividerFive" />
    <TextView
        android:id="@+id/tv_addVitamins_vitB12"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:paddingVertical="8dp"

        android:text="vitamin B12:"
        android:textSize="18dp"

        app:layout_constraintStart_toStartOf="@+id/vit_guideline"
        app:layout_constraintTop_toBottomOf="@+id/vit_dividerSix" />

    <EditText
        android:id="@+id/edit_addVitamins_vitA"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:paddingVertical="8dp"

        android:hint="µg"
        android:inputType="number"
        android:background="@null"

        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="@id/tv_addVitamins_vitA"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/tv_addVitamins_vitA"
        />

    <EditText
        android:id="@+id/edit_addVitamins_vitC"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:paddingVertical="8dp"

        android:hint="µg"
        android:inputType="number"
        android:background="@null"

        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="@id/tv_addVitamins_vitC"
        app:layout_constraintStart_toEndOf="@+id/tv_addVitamins_vitC"
        app:layout_constraintHorizontal_bias="0.5"
        />
    <EditText
        android:id="@+id/edit_addVitamins_vitD"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:paddingVertical="8dp"

        android:hint="µg"
        android:inputType="number"
        android:background="@null"

        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="@id/tv_addVitamins_vitD"
        app:layout_constraintStart_toEndOf="@+id/tv_addVitamins_vitD"
        app:layout_constraintHorizontal_bias="0.5"
        />
    <EditText
        android:id="@+id/edit_addVitamins_vitE"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:paddingVertical="8dp"

        android:hint="µg"
        android:inputType="number"
        android:background="@null"

        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="@id/tv_addVitamins_vitE"
        app:layout_constraintStart_toEndOf="@+id/tv_addVitamins_vitE"
        app:layout_constraintHorizontal_bias="0.5"
        tools:layout_editor_absoluteY="267dp"
        />

    <EditText
        android:id="@+id/edit_addVitamins_vitK"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="@null"

        android:hint="µg"
        android:inputType="number"
        android:paddingVertical="8dp"

        app:layout_constraintBottom_toBottomOf="@id/tv_addVitamins_vitK"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/tv_addVitamins_vitK" />
    <EditText
        android:id="@+id/edit_addVitamins_vitB6"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="@null"

        android:hint="µg"
        android:inputType="number"
        android:paddingVertical="8dp"

        app:layout_constraintBottom_toBottomOf="@id/tv_addVitamins_vitB6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/tv_addVitamins_vitB6" />
    <EditText
        android:id="@+id/edit_addVitamins_vitB12"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="@null"

        android:hint="µg"
        android:inputType="number"
        android:paddingVertical="8dp"

        app:layout_constraintBottom_toBottomOf="@id/tv_addVitamins_vitB12"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/tv_addVitamins_vitB12" />

    <View
        android:id="@+id/vit_dividerOne"
        android:layout_width="wrap_content"
        android:layout_height="1dp"
        android:background="@android:color/darker_gray"
        app:layout_constraintStart_toEndOf="@id/vit_guideline"
        app:layout_constraintTop_toBottomOf="@id/tv_addVitamins_vitA"
        />
    <View
        android:id="@+id/vit_dividerTwo"
        android:layout_width="wrap_content"
        android:layout_height="1dp"
        android:background="@android:color/darker_gray"
        app:layout_constraintStart_toEndOf="@id/vit_guideline"
        app:layout_constraintTop_toBottomOf="@id/tv_addVitamins_vitC"
        />
    <View
        android:id="@+id/vit_dividerThree"
        android:layout_width="wrap_content"
        android:layout_height="1dp"
        android:background="@android:color/darker_gray"
        app:layout_constraintStart_toEndOf="@id/vit_guideline"
        app:layout_constraintTop_toBottomOf="@id/tv_addVitamins_vitD"
        />
    <View
        android:id="@+id/vit_dividerFour"
        android:layout_width="wrap_content"
        android:layout_height="1dp"
        android:background="@android:color/darker_gray"
        app:layout_constraintStart_toEndOf="@id/vit_guideline"
        app:layout_constraintTop_toBottomOf="@id/tv_addVitamins_vitE"
        />
    <View
        android:id="@+id/vit_dividerFive"
        android:layout_width="wrap_content"
        android:layout_height="1dp"
        android:background="@android:color/darker_gray"
        app:layout_constraintStart_toEndOf="@id/vit_guideline"
        app:layout_constraintTop_toBottomOf="@id/tv_addVitamins_vitK"
        />
    <View
        android:id="@+id/vit_dividerSix"
        android:layout_width="wrap_content"
        android:layout_height="1dp"
        android:background="@android:color/darker_gray"
        app:layout_constraintStart_toEndOf="@id/vit_guideline"
        app:layout_constraintTop_toBottomOf="@id/tv_addVitamins_vitB6"
        />
    <View
        android:id="@+id/vit_dividerSeven"
        android:layout_width="wrap_content"
        android:layout_height="1dp"
        android:background="@android:color/darker_gray"
        app:layout_constraintStart_toEndOf="@id/vit_guideline"
        app:layout_constraintTop_toBottomOf="@id/tv_addVitamins_vitB12"
        />

    <ImageButton
        android:id="@+id/btn_minerals_done"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_done_black_24dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints"

        app:layout_constraintVertical_bias="0.75"
        app:layout_constraintHorizontal_bias="0.75"/>

    <androidx.constraintlayout.widget.Barrier
        android:id="@+id/vit_barrier"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:barrierDirection="right"
        app:constraint_referenced_ids="tv_addVitamins_vitA,tv_addVitamins_vitC,vit_guideline"
        tools:layout_editor_absoluteX="411dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/vit_guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="16dp" />

</androidx.constraintlayout.widget.ConstraintLayout>