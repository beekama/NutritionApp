<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/startPageFragment">

    <fragment
        android:id="@+id/startPageFragment"
        android:name="com.example.nutritionapp.ui.StartPageFragment"
        android:label="StartPageFragment" >
        <action
            android:id="@+id/action_startPageFragment_to_aboutPageFragment"
            app:destination="@id/aboutPageFragment" />
        <action
            android:id="@+id/action_startPageFragment_to_weightTrackingFragment"
            app:destination="@id/weightTrackingFragment" />
        <action
            android:id="@+id/action_startPageFragment_to_configurationFragment"
            app:destination="@id/configurationFragment" />
        <action
            android:id="@+id/action_startPageFragment_to_journalFragment"
            app:destination="@id/journalFragment" />
        <action
            android:id="@+id/action_startPageFragment_to_aboutPageFragment2"
            app:destination="@id/aboutPageFragment" />
        <action
            android:id="@+id/action_startPageFragment_to_customFoodFragment"
            app:destination="@id/customFoodFragment" />
        <action
            android:id="@+id/action_startPageFragment_to_foodGroupFragment"
            app:destination="@id/foodGroupFragment" />
        <action
            android:id="@+id/action_startPageFragment_to_recommendationFragment"
            app:destination="@id/recommendationFragment" />
    </fragment>
    <fragment
        android:id="@+id/aboutPageFragment"
        android:name="com.example.nutritionapp.ui.AboutPageFragment"
        android:label="About"
        tools:layout="@layout/fragment_about_page" >
        <action
            android:id="@+id/action_aboutPageFragment_to_startPageFragment"
            app:destination="@id/startPageFragment" />
        <action
            android:id="@+id/action_aboutPageFragment_to_weightTrackingFragment"
            app:destination="@id/weightTrackingFragment" />
        <action
            android:id="@+id/action_aboutPageFragment_to_configurationFragment"
            app:destination="@id/configurationFragment" />
        <action
            android:id="@+id/action_aboutPageFragment_to_journalFragment"
            app:destination="@id/journalFragment" />
        <action
            android:id="@+id/action_aboutPageFragment_to_customFoodFragment"
            app:destination="@id/customFoodFragment" />
        <action
            android:id="@+id/action_aboutPageFragment_to_recommendationFragment"
            app:destination="@id/recommendationFragment" />
    </fragment>
    <fragment
        android:id="@+id/weightTrackingFragment"
        android:name="com.example.nutritionapp.ui.WeightTrackingFragment"
        android:label="fragment_weight_tracking"
        tools:layout="@layout/fragment_weight_tracking" >
        <action
            android:id="@+id/action_weightTrackingFragment_to_aboutPageFragment"
            app:destination="@id/aboutPageFragment" />
        <action
            android:id="@+id/action_weightTrackingFragment_to_startPageFragment"
            app:destination="@id/startPageFragment" />
        <action
            android:id="@+id/action_weightTrackingFragment_to_configurationFragment"
            app:destination="@id/configurationFragment" />
        <action
            android:id="@+id/action_weightTrackingFragment_to_journalFragment"
            app:destination="@id/journalFragment" />
        <action
            android:id="@+id/action_weightTrackingFragment_to_journalFragment2"
            app:destination="@id/journalFragment" />
        <action
            android:id="@+id/action_weightTrackingFragment_to_journalFragment3"
            app:destination="@id/journalFragment" />
        <action
            android:id="@+id/action_weightTrackingFragment_to_customFoodFragment"
            app:destination="@id/customFoodFragment" />
        <action
            android:id="@+id/action_weightTrackingFragment_to_recommendationFragment"
            app:destination="@id/recommendationFragment" />
    </fragment>
    <fragment
        android:id="@+id/configurationFragment"
        android:name="com.example.nutritionapp.ui.ConfigurationFragment"
        android:label="fragment_configuration"
        tools:layout="@layout/fragment_configuration" >
        <action
            android:id="@+id/action_configurationFragment_to_startPageFragment"
            app:destination="@id/startPageFragment" />
        <action
            android:id="@+id/action_configurationFragment_to_aboutPageFragment"
            app:destination="@id/aboutPageFragment" />
        <action
            android:id="@+id/action_configurationFragment_to_journalFragment"
            app:destination="@id/journalFragment" />
        <action
            android:id="@+id/action_configurationFragment_to_customFoodFragment"
            app:destination="@id/customFoodFragment" />
        <action
            android:id="@+id/action_configurationFragment_to_recommendationFragment"
            app:destination="@id/recommendationFragment" />
    </fragment>
    <fragment
        android:id="@+id/journalFragment"
        android:name="com.example.nutritionapp.ui.JournalFragment"
        android:label="fragment_journal"
        tools:layout="@layout/fragment_journal" >
        <action
            android:id="@+id/action_journalFragment_to_startPageFragment"
            app:destination="@id/startPageFragment" />
        <action
            android:id="@+id/action_journalFragment_to_configurationFragment"
            app:destination="@id/configurationFragment" />
        <action
            android:id="@+id/action_journalFragment_to_weightTrackingFragment"
            app:destination="@id/weightTrackingFragment" />
        <action
            android:id="@+id/action_journalFragment_to_aboutPageFragment"
            app:destination="@id/aboutPageFragment" />
        <action
            android:id="@+id/action_journalFragment_to_customFoodFragment"
            app:destination="@id/customFoodFragment" />
        <action
            android:id="@+id/action_journalFragment_to_foodGroupFragment"
            app:destination="@id/foodGroupFragment" />
        <action
            android:id="@+id/action_journalFragment_to_recommendationFragment"
            app:destination="@id/recommendationFragment" />
    </fragment>
    <fragment
        android:id="@+id/customFoodFragment"
        android:name="com.example.nutritionapp.ui.CustomFoodFragment"
        android:label="fragment_custom_food"
        tools:layout="@layout/fragment_create_food" >
        <action
            android:id="@+id/action_customFoodFragment_to_startPageFragment"
            app:destination="@id/startPageFragment" />
        <action
            android:id="@+id/action_customFoodFragment_to_aboutPageFragment"
            app:destination="@id/aboutPageFragment" />
        <action
            android:id="@+id/action_customFoodFragment_to_weightTrackingFragment"
            app:destination="@id/weightTrackingFragment" />
        <action
            android:id="@+id/action_customFoodFragment_to_configurationFragment"
            app:destination="@id/configurationFragment" />
        <action
            android:id="@+id/action_customFoodFragment_to_journalFragment"
            app:destination="@id/journalFragment" />
        <action
            android:id="@+id/action_customFoodFragment_to_createFoodItemFragment"
            app:destination="@id/createFoodItemFragment" />
        <action
            android:id="@+id/action_customFoodFragment_to_recommendationFragment"
            app:destination="@id/recommendationFragment" />
    </fragment>
    <fragment
        android:id="@+id/createFoodItemFragment"
        android:name="com.example.nutritionapp.ui.CreateFoodItemFragment"
        android:label="fragment_create_food_item"
        tools:layout="@layout/fragment_create_food_item" >
        <action
            android:id="@+id/action_createFoodItemFragment_to_customFoodFragment"
            app:destination="@id/customFoodFragment" />
        <argument
            android:name="fdc_id"
            app:argType="string"
            app:nullable="true" />
    </fragment>
    <fragment
        android:id="@+id/foodGroupFragment"
        android:name="com.example.nutritionapp.ui.FoodGroupFragment"
        android:label="fragment_food_group"
        tools:layout="@layout/fragment_food_group" >
        <action
            android:id="@+id/action_foodGroupFragment_to_journalFragment"
            app:destination="@id/journalFragment" />
    </fragment>
    <fragment
        android:id="@+id/recommendationFragment"
        android:name="com.example.nutritionapp.ui.RecommendationFragment"
        android:label="fragment_recommendation"
        tools:layout="@layout/fragment_recommendation" >
        <action
            android:id="@+id/action_recommendationFragment_to_startPageFragment"
            app:destination="@id/startPageFragment" />
        <action
            android:id="@+id/action_recommendationFragment_to_aboutPageFragment"
            app:destination="@id/aboutPageFragment" />
        <action
            android:id="@+id/action_recommendationFragment_to_weightTrackingFragment"
            app:destination="@id/weightTrackingFragment" />
        <action
            android:id="@+id/action_recommendationFragment_to_configurationFragment"
            app:destination="@id/configurationFragment" />
        <action
            android:id="@+id/action_recommendationFragment_to_journalFragment"
            app:destination="@id/journalFragment" />
        <action
            android:id="@+id/action_recommendationFragment_to_customFoodFragment"
            app:destination="@id/customFoodFragment" />
        <argument
            android:name="startDate"
            app:argType="java.time.LocalDate"
            app:nullable="true" />
        <action
            android:id="@+id/action_recommendationFragment_to_recommendationElementFragment"
            app:destination="@id/recommendationElementFragment" />
    </fragment>
    <fragment
        android:id="@+id/recommendationElementFragment"
        android:name="com.example.nutritionapp.ui.RecommendationNutritionElementFragment"
        android:label="fragment_recommendation_element"
        tools:layout="@layout/fragment_recommendation_nutrition_element" >
        <action
            android:id="@+id/action_recommendationElementFragment_to_recommendationFragment"
            app:destination="@id/recommendationFragment" />
        <argument
            android:name="nutritionElement"
            app:argType="com.example.nutritionapp.other.NutritionElement"
            app:nullable="true" />
    </fragment>
</navigation>