create TABLE person(
    "gender" TEXT,
    "age"    INTEGER,
    "height" INTEGER,
    "weight" INTEGER,
    "energyTarget" INTEGER
);

create TABLE foodlog(
    "food_id"   TEXT,
    "date"  INTEGER,
    "group_id"  TEXT,
    "amountInGram" INTEGER,
    "loggedAt"  INTEGER
);

.mode csv
.import food.csv food
.import food_portion.csv food_portion
.import nutrient.csv nutrient

